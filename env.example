# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_KEY=your-service-role-key

# LLM Service Configuration
LLM_SERVICE_URL=http://localhost:8003

# Worker Configuration
WORKER_MODE=ingestion
BATCH_SIZE=10
LOG_LEVEL=info

# System User ID for feeds
SYSTEM_USER_ID=system-ingestion

# RSS Ingestion Settings (optional)
RSS_REQUEST_TIMEOUT=30
RSS_MAX_RETRIES=3

# HTML Extraction Settings (optional)
HTML_REQUEST_TIMEOUT=30
HTML_MAX_RETRIES=3
HTML_USER_AGENT=Mozilla/5.0 (compatible; XynenyxBot/1.0)

# Chunking Settings (optional)
CHUNK_SIZE=512
CHUNK_OVERLAP=50

# Embedding Settings (optional)
EMBEDDING_BATCH_SIZE=10
EMBEDDING_MAX_RETRIES=3
EMBEDDING_RETRY_DELAY=1.0
LLM_SERVICE_TIMEOUT=60
